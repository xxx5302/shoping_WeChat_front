.modal-content {
  position: relative;
  background-color: #fff;
  box-sizing: border-box;
  border-radius: 10rpx;
  font-size: 28rpx;
}

.modal-head {
  position: relative;
  padding: 20rpx 30rpx;
}

.modal-foot {
  position: relative;
  padding: 20rpx 30rpx;
  /* border-bottom:1rpx solid #efefef; */
}

.modal-head::after,.modal-foot::before {
  content: '';
  position: absolute;
  left: 0;
  right: 0;
  height: 1px;
	transform: scaleY(.5);
}

.modal-head::after {
  border-bottom: 1px solid #c8c7cc;
  bottom: 0;
}

.modal-foot::before {
  border-top: 1px solid #c8c7cc;
  top: 0;
}

.modal-foot .tot, .modal-head-r {
  color: #ff5344;
}

.modal-head-l {
